    "def is_power(powered_number, base_number):\n",
    "    n = base_number\n",
    "    power_counter = 1\n",
    "    while n<powered_number:\n",
    "        n=n*n\n",
    "        power_counter+=1\n",
    "    if n==powered_number:\n",
    "        return \"true\"\n",
    "    else:\n",
    "        return \"false\""
   ]
